upstream django{
	server unix:///home/krishna-chaitanya/projects/django_projects/speed_copy/speed_copy.sock;
}

server{
	listen 80;
	server_name 0.0.0.0;
	charset utf-8;
	#max upload size
	client_max_body_size 75M;
	#static
	location /static{
		alias /home/krishna-chaitanya/projects/django_projects/speed_copy/static;
	}
	location /media{
		alias /home/krishna-chaitanya/projects/django_projects/speed_cop
y/media;
	}
	location / {
		uwsgi_pass django;
		include /home/krishna-chaitanya/projects/django_projects/speed_copy/uwsgi_params;
	}
}
